<h1>Upload a new photo</h1>

<%= form_for @photo do |f| %>
  <div class="form-group">
    <%= f.label :title do %>
      Title
      <%= f.text_field :title %>
    <% end %>
  </div>

  <div class="form-group">
    <%= f.label :photo do %>
      Photo
      <%= f.file_field :photo %>
    <% end %>
  </div>

  <%= f.submit %>
<% end %>
